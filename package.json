{
  "name": "datahub-io",
  "version": "0.1.3",
  "private": true,
  "proxy": "https://datahub.ki.agh.edu.pl",
  "author": {
    "name": "Jakub Janicki, Dawid Pastuszka, Michał Stencel, Maciej Kazalski, Bartłomiej Zapart",
    "email": "no_one@agh.edu.pl"
  },
  "description": "Aplikacja do wizualizacji danych z datahub",
  "homepage": "./",
  "main": "./public/electron.js",
  "dependencies": {
    "@date-io/date-fns": "^2.13.2",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@material-ui/core": "^5.0.0-beta.5",
    "@material-ui/lab": "^5.0.0-alpha.44",
    "@material-ui/styles": "^5.0.0-beta.5",
    "@mui/icons-material": "^5.6.2",
    "@mui/lab": "^5.0.0-alpha.78",
    "@mui/material": "^5.6.2",
    "@mui/system": "^5.2.3",
    "@mui/x-data-grid": "^5.11.0",
    "@mui/x-date-pickers": "^5.0.0-alpha.2",
    "@mui/x-date-pickers-pro": "^5.0.0-alpha.2",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "assert": "^2.0.0",
    "async": "^3.2.3",
    "axios": "^0.26.1",
    "bootstrap": "^5.1.3",
    "buffer": "^6.0.3",
    "crypto": "^1.0.1",
    "crypto-browserify": "^3.12.0",
    "date-fns": "^2.28.0",
    "dotenv": "^10.0.0",
    "eslint-config-react-app": "^7.0.1",
    "fb-watchman": "^2.0.1",
    "https": "^1.0.0",
    "https-browserify": "^1.0.0",
    "notistack": "^2.0.5",
    "os": "^0.1.2",
    "path": "^0.12.7",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-redux": "^7.2.8",
    "react-scripts": "5.0.0",
    "react-swipeable-views": "^0.14.0",
    "recharts": "^2.1.9",
    "redux": "^4.1.2",
    "redux-persist": "^6.0.0",
    "stream": "0.0.2",
    "stream-http": "^3.2.0",
    "util": "^0.12.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:start": "concurrently -k \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electronmon .\"",
    "electron:package:mac": "yarn build && electron-builder -m -c.extraMetadata.main=build/electron.js",
    "electron:package:win": "yarn build && electron-builder -w -c.extraMetadata.main=build/electron.js",
    "electron:package:linux": "yarn build && electron-builder -l -c.extraMetadata.main=build/electron.js"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "electron": "^13.1.9",
    "electron-builder": "^22.11.7",
    "electronmon": "^2.0.1",
    "wait-on": "^6.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "last 1 electron version"
    ],
    "development": [
      "last 1 electron version"
    ]
  },
  "build": {
    "appId": "com.electron.datahubIO",
    "productName": "DatahubIO",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "deb"
    }
  }
}